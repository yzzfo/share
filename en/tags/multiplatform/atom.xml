        <published>2024-11-29T21:21:05+00:00</published>
        <updated>2024-11-30T13:25:17+00:00</updated>
        
        <author>
          <name>
            
              yzzfo
            
          </name>
        </author>
        
        <link rel="alternate" type="text/html" href="https://yzzfo.github.io/en/unity/store/easy-save-the-complete-save-game-data-serializer-system/"/>
